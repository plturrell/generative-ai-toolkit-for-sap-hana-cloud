# Full stack deployment configuration for SAP BTP

# Deployment mode
DEPLOYMENT_MODE=full

# Application settings
APPLICATION_NAME=hana-ai-toolkit
PORT=8080
NODE_ENV=production
LOG_LEVEL=info

# CORS settings
CORS_ORIGINS=https://*.hana.ondemand.com

# Backend settings
DEFAULT_LLM_MODEL=gpt-4o
MAX_TOKENS=8192
TEMPERATURE=0.7

# HANA database connection
HANA_HOST=${HANA_HOST}
HANA_PORT=${HANA_PORT}
HANA_USER=${HANA_USER}
HANA_PASSWORD=${HANA_PASSWORD}

# API security
API_KEYS=${API_KEYS}

# GPU optimization
NVIDIA_VISIBLE_DEVICES=none
ENABLE_GPU=false
CPU_THREADS=4

# Health check
HEALTH_CHECK_INTERVAL=60
ENABLE_BACKEND_HEALTH_CHECKS=true